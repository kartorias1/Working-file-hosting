  "custom-commands": [
    "mkdir /temp",
    "git clone https://github.com/kartorias1/Archiso_dependencies.git /temp/Archiso_dependencies",
    "cd /temp/Archiso_dependencies/AUR_packages && yes | pacman -U *",
    "git clone https://github.com/kartorias1/Hyprland-dotfiles.git /temp/Hyprland-dotfiles",
    "cp -r /temp/Hyprland-dotfiles/hypr/ /home/$(ls /home | head -1)/.config/",
    "cp -r /temp/Hyprland-dotfiles/kitty/ /home/$(ls /home | head -1)/.config/",
    "cp -r /temp/Hyprland-dotfiles/starship/ /home/$(ls /home | head -1)/.config/",
    "cp -r /temp/Hyprland-dotfiles/cava/ /home/$(ls /home | head -1)/.config/",
    "cp -r /temp/Hyprland-dotfiles/bat/ /home/$(ls /home | head -1)/.config/",
    "cp -r /temp/Hyprland-dotfiles/tmux/ /home/$(ls /home | head -1)/.config/",
    "cp -r /temp/Hyprland-dotfiles/waybar/ /home/$(ls /home | head -1)/.config/",
    "cp -r /temp/Hyprland-dotfiles/backup /home/$(ls /home | head -1)/",
    "mkdir -p mkdir /home/$(ls /home | head -1)/.local/share",
    "cp -r /temp/Hyprland-dotfiles/backup/fonts/ /home/$(ls /home | head -1)/.local/share/",
    "cp /temp/Hyprland-dotfiles/.zshrc_without_vi_mode /home/$(ls /home | head -1)/.zshrc",
    "cd /home/$(ls /home | head -1) && chown $(ls /home | head -1) .zshrc backup .local .local/share .local/share/fonts",
    "cd /home/$(ls /home | head -1)/.config && chown $(ls /home | head -1) hypr kitty starship cava bat tmux waybar",
    "systemctl enable sddm",
    "chsh -s $(which zsh) $(ls /home | head -1)",
    "mkdir -p /usr/share/sddm/themes",
    "cp -r /temp/Archiso_dependencies/sugar-candy /usr/share/sddm/themes/",
    "cp /temp/Archiso_dependencies/sddm.conf /etc/"
  ],
